{
  "name": "hap-node-client",
  "version": "0.2.7",
  "description": "Client for Hap-NodeJS",
  "main": "HAPNodeJSClient.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "cd scripts; node gen-hap-types.js",
    "test": "nodemon",
    "test-coverage": "jest --coverage"
  },
  "devDependencies": {
    "documentation": "^14.0.3",
    "hap-nodejs": "^0.11.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "semver": "^7.6.0"
  },
  "dependencies": {
    "axios": "1.6.8",
    "axios-retry": "4.1.0",
    "better-queue": "3.8.12",
    "bonjour-hap": "3.7.1",
    "buffer": "6.0.3",
    "debug": "4.3.4",
    "extend": "3.0.2",
    "ip": "2.0.1",
    "once": "1.4.0"
  },
  "nodemonConfig": {
    "watch": [],
    "ext": "js",
    "ignore": [],
    "exec": "jest",
    "signal": "SIGTERM",
    "env": {
      "NODE_OPTIONS": "--trace-warnings"
    }
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorthernMan54/hap-node-client.git"
  },
  "author": "NorthernMan54",
  "license": "ISC"
}
