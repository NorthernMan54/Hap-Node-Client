{
  "name": "hap-node-client",
  "version": "0.2.4",
  "description": "Client for Hap-NodeJS",
  "main": "HAPNodeJSClient.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "cd scripts; node gen-hap-types.js",
    "test": "nodemon",
    "test-coverage": "jest --coverage"
  },
  "devDependencies": {
    "documentation": "^14.0.2",
    "hap-nodejs": "0.11.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "axios-retry": "^3.9.1",
    "better-queue": ">=3.8.12",
    "bonjour-hap": "^3.6.4",
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "extend": "^3.0.2",
    "ip": "^1.1.8",
    "once": ">=1.4.0"
  },
  "nodemonConfig": {
    "watch": [],
    "ext": "js",
    "ignore": [],
    "exec": "jest",
    "signal": "SIGTERM",
    "env": {
      "NODE_OPTIONS": "--trace-warnings"
    }
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorthernMan54/hap-node-client.git"
  },
  "author": "NorthernMan54",
  "license": "ISC"
}
